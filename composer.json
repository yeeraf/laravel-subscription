{
    "name": "yeeraf/laravel-subscription",
    "description": "A Laravel package for create subscription service with different benefits.",
    "type": "library",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Yeeraf\\LaravelSubscription\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yeeraf\\LaravelSubscription\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "derflohwalzer",
            "email": "tanaphon.wat@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Yeeraf\\LaravelSubscription\\PackageServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
